
	<table summary="Place and Card form fields">
		<tr>
			<th>Name</th>
			<td><%= text_field(:place, :name) %></td>
		</tr>
		<tr>
			<th>Type</th>
			<td><%= text_field(:place, :location_type) %></td>
		</tr>
		<tr>
			<th>City</th>
			<td><%= text_field(:place, :city) %></td>
		</tr>
		<tr>
			<th>Country</th>
			<td><%= text_field(:place, :country) %></td>
		</tr>
		<tr>
			<th>Description</th>
			<td><%= text_field(:card, :text) %></td>
		</tr>
	</table>
